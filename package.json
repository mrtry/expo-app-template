{
  "name": "expo-app-template",
  "main": "index.ts",
  "version": "1.0.0",
  "scripts": {
    "start": "yarn start:app:development",
    "start:app:development": "zx scripts/watch-i18n-background.mjs && APP_VARIANT=development expo start",
    "start:app:staging": "zx scripts/watch-i18n-background.mjs && APP_VARIANT=staging expo start",
    "start:app:production": "zx scripts/watch-i18n-background.mjs && APP_VARIANT=production expo start",
    "start:storybook": "zx scripts/watch-i18n-background.mjs && STORYBOOK_ENABLED='true' expo start",
    "build:dev:android:local": "expo run:android",
    "build:dev:ios:local": "expo run:ios",
    "lint": "run-s typecheck lint:eslint lint:prettier",
    "lint:eslint": "eslint .",
    "lint:dependency-graph": "depcruise src --config .dependency-cruiser.js",
    "lint:prettier": "prettier --check './**/*.{js,jsx,ts,tsx,json}'",
    "lint:fix": "run-s lint:eslint:fix lint:prettier:fix",
    "lint:eslint:fix": "eslint . --fix",
    "lint:prettier:fix": "prettier --write './**/*.{js,jsx,ts,tsx,json}'",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "jest",
    "typesafe-i18n": "typesafe-i18n",
    "postinstall": "husky && sb-rn-get-stories && typesafe-i18n --no-watch"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@formatjs/intl-getcanonicallocales": "^2.5.5",
    "@formatjs/intl-locale": "^4.2.11",
    "@formatjs/intl-pluralrules": "^5.4.4",
    "@react-native-vector-icons/material-design-icons": "^12.0.1",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@tanstack/react-query": "^5.83.0",
    "expo": "~53.0.12",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.2.4",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.3.0",
    "expo-linking": "~7.1.5",
    "expo-router": "~5.1.0",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.9",
    "expo-web-browser": "~14.2.0",
    "fs-extra": "^11.3.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.4",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-webview": "13.13.5",
    "typesafe-i18n": "^5.26.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@eslint-react/eslint-plugin": "^1.52.2",
    "@gorhom/bottom-sheet": "^5.1.6",
    "@prettier/plugin-oxc": "^0.0.4",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.4.2",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native-community/slider": "^4.5.7",
    "@storybook/addon-ondevice-actions": "^9.0.14",
    "@storybook/addon-ondevice-controls": "^9.0.14",
    "@storybook/react": "^9.0.16",
    "@storybook/react-native": "^9.0.14",
    "@storybook/test": "^8.6.14",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.13",
    "@types/react": "~19.0.10",
    "babel-loader": "^8.4.1",
    "dependency-cruiser": "^16.10.3",
    "eslint": "^9.29.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-check-file": "^3.3.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "jest-expo": "^53.0.9",
    "lint-staged": "^16.1.2",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.6.0",
    "react-native-svg": "^15.12.0",
    "storybook": "^9.0.15",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "zx": "^8.6.1"
  },
  "private": true,
  "packageManager": "yarn@4.5.1"
}
